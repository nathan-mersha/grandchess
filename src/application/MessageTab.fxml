<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="114.0" minWidth="251.0" prefHeight="114.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controler_Message">
   <children>
      <Label alignment="CENTER" layoutX="1.0" layoutY="8.0" prefHeight="27.0" prefWidth="251.0" text="Your Time is up" textFill="#0e5e96">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lostPlayerName" alignment="CENTER" layoutY="38.0" prefHeight="27.0" prefWidth="251.0" text="LostPlayer" textFill="#0e5e96">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="exit" layoutX="10.0" layoutY="79.0" mnemonicParsing="false" onAction="#onButtonAction" prefHeight="27.0" prefWidth="110.0" text="Exit" textFill="#0e5e96" />
      <Button fx:id="rematch" layoutX="130.0" layoutY="79.0" mnemonicParsing="false" onAction="#onButtonAction" prefHeight="27.0" prefWidth="110.0" text="Re-Match" textFill="#0e5e96" />
   </children>
</AnchorPane>
